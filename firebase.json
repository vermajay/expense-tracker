{
  "hosting": {
    "site": "trackmyexpenses",
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "reserved": [
      {
        "site": "trackmyexpenses",
        "domain": "trackmyexpenses.web.app"
      },
      {
        "site": "trackmyexpenses",
        "domain": "trackmyexpenses.firebaseapp.com"
      }
    ]
  }
}